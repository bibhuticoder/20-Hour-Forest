<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid
                android:color="@color/cpb_front"></solid>
            <stroke
                android:color="@color/cpb_back"
                android:width="1dp"></stroke>
            <corners
                android:radius="5dp"></corners>
            <padding
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"
                android:bottom="10dp"></padding>
            <size android:height="100dp"
                android:width="200dp"></size>


            <shape>
                <solid
                    android:color="@color/cpb_back"></solid>

                <corners
                    android:radius="5dp"></corners>
                <padding
                    android:left="10dp"
                    android:right="10dp"
                    android:top="10dp"
                    android:bottom="10dp"></padding>
                <size android:height="90dp"
                    android:width="200dp"></size>

            </shape>

        </shape>

    </item>



</selector>